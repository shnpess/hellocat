<div class="login-page">
  <div class="login__form">
  <h2>LOG IN</h2>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %> 
      <%= f.email_field :email, autofocus: true, class: "field", placeholder: "Email",autocomplete: "email" %>
      <%= f.password_field :password, class: "field", placeholder: "Password", autocomplete: "current-password" %>
      <%= f.submit "Log in", class: "form__submit" %>        
    <% end %>
  <%= render "devise/shared/links" %>
 </div>
</div>





  
    
   

  